[run]
source = code_agent
omit =
    tests/*
    */__pycache__/*
    */site-packages/*
    */.venv/*
    code_agent/agent/custom_agent/*
    code_agent/agent/cli_runner/*
    code_agent/agent/software_engineer/*
    code_agent/agent/travel-concierge/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
fail_under = 80

[html]
directory = htmlcov
